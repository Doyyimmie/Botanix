# Discord bot token
TOKEN=YOUR_BOT_TOKEN_HERE

# MongoDB connection URI
MONGO_URI=mongodb://mongo:27017/botanix

# Discord client ID (for slash commands registration)
CLIENT_ID=YOUR_CLIENT_ID_HERE

# Optional: log channel ID in your server
LOG_CHANNEL_ID=YOUR_LOG_CHANNEL_ID_HERE

# Optional: prefix for prefix commands
PREFIX=!

# Optional: shard count (if using sharding)
SHARD_COUNT=1
